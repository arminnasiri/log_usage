{
    "name": "arminhanis/log-usage",
    "description": "log usage service",
    "keywords": [
        "armin",
        "log-usage"
    ],
    "homepage": "https://github.com/arminnasiri/log-usage",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Armin Nasiri",
            "email": "a.nasiri@map.ir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "nmred/kafka-php": "0.2.*",
        "psr/log": "1.0.2"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Shiveh\\LogUsage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shiveh\\LogUsage\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Shiveh\\LogUsage\\LogUsageServiceProvider"
            ],
            "aliases": {
                "LogUsage": "Shiveh\\LogUsage\\LogUsageFacade"
            }
        }
    }
}
